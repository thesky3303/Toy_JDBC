	<!-- 할 일 전체 조회 -->
	<entry key="stdListFullView">
		SELECT 
			STD_NO, 
			STD_NAME, 
			STD_AGE, 
			STD_GENDER,
			STD_SCORE
		FROM TB_STUDENT
	</entry>
	
		<!-- 할 일 추가 -->
	<entry key="stdAdd">
		INSERT INTO TB_STUDENT
		VALUES(SEQ_STD_NO.NEXTVAL, ?, ?, ?, ?)
	</entry>
	
	<entry key="stdAdd">
		SELECT 
			STD_NO, 
			STD_NAME, 
			STD_AGE, 
			STD_GENDER,
			STD_SCORE
		FROM TB_STUDENT
		WHERE STD_NO = ?
	</entry>
	
	
	
	
